    <select id="queryItemValues" resultType="map">
        SELECT
        PK_PAYFILE_DOC,
        PK_PAYFILE,
        STAFF_ID,
        <if test="columns!=null and columns !=''">
            ${columns},
        </if>
        CREATOR,
        CREATIONTIME,
        MODIFIER,
        MODIFIEDTIME,
        TS,
        DR,
        MEMO
        FROM
        ${tableName}
        WHERE TENANTID = #{cond.tenantId}
